{
  "name": "Block the youtube feed",
  "description": "Remove the feeds from youtube and just leave the search bar",
  "icons": {"128": "icon.png"},
  "version": "1.6.5",
  "permissions": [
      "tabs", "http://*.youtube.com/*", "https://*.youtube.com/*"
  ],
  "browser_action": {
      "default_title": "Removed the recommended videos sections",
      "default_icon": "icon.png"
  },
    "content_scripts": [{
        "css": ["style.css"],
        "js": [],
        "matches": ["*://www.youtube.com/", "*://www.youtube.com/watch*", "*://www.youtube.com/*", "*://www.youtube.com/user*"],
        "all_frames": true,
        "run_at": "document_start"
    }],

  "manifest_version": 2
}
